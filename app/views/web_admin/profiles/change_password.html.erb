<%= form_for @web_admin, :url=> reset_password_web_admin_profile_path(:id=> current_web_admin.id), :method => "put" do |f| %>
  <%= f.password_field :current_password, :autocomplete => "off", class: "sitewide-input", placeholder: "Old Password" %> &nbsp;
    <%=f.password_field :password,:placeholder=>"New Password"%> &nbsp; 
    <%=f.password_field :password_confirmation,:placeholder=>"Re-Type Password"%> 
    <%=f.submit "Update"%>
  <%end%>
<br/> <br/>
<%= form_for @web_admin, :url=> reset_email_web_admin_profile_path(:id=> current_web_admin.id), :method => "put" do |f| %>
  <%= f.email_field :email %>&nbsp;  &nbsp;  &nbsp; &nbsp;
 
    <%=f.submit "Update"%>
  <%end%>
