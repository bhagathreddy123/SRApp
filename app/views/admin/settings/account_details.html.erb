<%= form_for @admin, :url=> admin_setting_path(@admin), :method => "put" do |f| %>


  <div>
    <%= f.text_field :school_name, :placeholder=>"Institute/College/SchoolName" %>
  </div>

  <br/>
  <div>
    <%= f.text_field :name, :placeholder => "FullName"%>
    <%= f.email_field :email, autofocus: true,:placeholder => "Email Id" %>
  </div>

  <br/>

  <div>
    <%= f.password_field :password, autocomplete: "off",:placeholder => "Password" %>
    <%= f.password_field :password_confirmation, autocomplete: "off",:placeholder => "Retype Password" %>
  </div>
  <br/>

  <div>  <%= f.text_field :address1, :placeholder=>"AddressLine1" %> </div>
  <br/>

  <div>
    <%= f.text_field :address2, :placeholder=>"AddressLine2" %>
    <%=f.text_field :country, :placeholder=>"Country" %>
  </div>
  <br/>
  <div>
    <%= f.text_field :city, :placeholder=>"City" %>
    <%= f.text_field :state, :placeholder=>"State" %>
    <%= f.text_field :zipcode, :placeholder=>"Pincode/Zipcode" %>
  </div>
  <br/>
  <div>
    <%=f.label :MobileNumber%>
    <%=f.text_field :mobile_no%>
  </div>
  <br>
  <%=f.submit%>
<%end%>

